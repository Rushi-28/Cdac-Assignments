package binarySearchTree;

public class Queue {
	QNode front,rear;
	public Queue()
	{
		front=rear=null;
	}
	public void enque(Node root)
	{
		Node n1=root;
		if(front==null)
			rear=front=n1;
		else
			{
				rear.next=n1;
				n1=rear;
			}
	}
	public boolean isEmpty()
	{
		if(rear==null)
			return true;
		else return false;
	}
	public QNode dequeue()
	{
		QNode data;
		data=front;
		if(front==rear)
			front=rear=null;
		else
			front=front.next;
		return data;
	}
}
