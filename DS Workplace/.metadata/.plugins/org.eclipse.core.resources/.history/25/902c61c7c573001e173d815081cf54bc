package IntLinkedlist;

public class LinkedList {
	Node head;

	public LinkedList() {
		head=null;
	}
	public void addNode(int data)
	{
		Node res=new Node(data);
		if(head==null)
			head=res;
		else {
			Node move;
			for(move=head;move.getNext()!=null;move=move.getNext())
			{
				if(move.getData()<res.getData() && move.getNext().getData()>res.getData())
				{
					move.setNext(res);
					break;
				}
			}
		}
	}
	
}
