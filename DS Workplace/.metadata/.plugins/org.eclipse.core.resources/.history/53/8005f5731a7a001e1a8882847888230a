package binarySearchTree;

public class Queue {
	QNode front,rear;
	public Queue()
	{
		front=rear=null;
	}
	public void enque(int data)
	{
		QNode n1=new QNode(data);
		if(front==null)
			rear=front=n1;
		else
			{
				rear.next=n1;
				n1=rear;
			}
	}
	public QNode dequeue()
	{
		QNode data=null;
		data=front.data;
		if(front==rear)
			front=rear=null;
		else
			front=front.next;
		return data;
	}
}
