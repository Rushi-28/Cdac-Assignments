package assi2;

public class Emp {
	public int eid;
	public String name;
	public float sal;
	public Emp(int eid, String name, float sal) {
		super();
		this.eid = eid;
		this.name = name;
		this.sal = sal;
	}
	@Override
	public String toString() {
		return "\nEmp [eid=" + eid + ", name=" + name + ", sal=" + sal + "]";
	}
	
}
